<div class='container'>
  <div class='header'>
    <div class='background-image'></div>
  </div>
  <div class='profile-pic'>
    <img style='height: 100%' src=<%= current_user.profile_url %> />
  </div>
  <div id='blog-title'>
    <h1 id='title'>Is It Just Me Or....</h1>
  </div>
  <div class='row'>
    <div class='col-md-2 user-panel'>
      <%= link_to 'New Post', new_user_post_path, class: 'outline-button new-post' %>
    </div>
    <div class='col-md-8'>
      <div class='panel panel-default'>
        <% current_user.posts.each do |post| %>
          <div class='post'>
            <h4><%= link_to post.title, post_path(post), class: 'post-title' %></h4>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
