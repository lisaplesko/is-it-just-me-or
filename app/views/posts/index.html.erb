<div class='container'>
  <div class='header'>
    <div class='background-image'></div>
  </div>
  <div class='profile-pic'>
    <img style='height: 100%' src=<%= current_user.profile_url %> />
  </div>
  <div id='blog-title'>
    <h1 id='title'>Is It Just Me Or....</h1>
  </div>
  <div class='blog-panel'>
    <%= link_to 'New Post', new_user_post_path, class: 'btn btn-default' %>
  </div>
  <% current_user.posts.each do |post| %>
    <div class='post'>
      <h3><%= link_to post.title, post_path(post) %></h3>
    </div>
  <% end %>
</div>
